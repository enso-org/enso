{
  "name": "enso4vscode",
  "displayName": "Enso Tools for VSCode",
  "description": "Support for Enso programming and development",
  "version": "0.1.0",
  "preview": true,
  "license": "SEE LICENSE IN dist/LICENSE",
  "publisher": "enso",
  "author": {
    "name": "Enso.org"
  },
  "homepage": "https://github.com/enso-org/enso/blob/develop/tools/enso4igv/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/enso-org/enso.git"
  },
  "bugs": {
    "url": "https://github.com/enso-org/enso/issues"
  },
  "engines": {
    "vscode": "^1.49.0"
  },
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "keywords": [
    "polyglot",
    "graalvm",
    "truffle"
  ],
  "activationEvents": [
    "onLanguage:enso"
  ],
  "main": "./dist/extension",
  "contributes": {
    "configuration": {},
    "commands": [],
    "viewsContainers": {},
    "viewsWelcome": [],
    "views": {},
    "menus": {},
    "languages": [
      {
        "id": "enso",
        "aliases": [
          "Enso"
        ],
        "extensions": [
          ".enso"
        ],
        "configuration": "./src/main/resources/org/enso/tools/enso4igv/enso.tmLanguage.json"
      }
    ],
    "grammars": [
      {
        "language": "enso",
        "scopeName": "source.enso",
        "path": "./src/main/resources/org/enso/tools/enso4igv/enso.tmLanguage.json"
      }
    ],
    "snippets": [],
    "breakpoints": [
      {
        "language": "enso"
      }
    ],
    "debuggers": []
  },
  "scripts": {
    "vsix": "vsce package",
    "vscode:prepublish": "webpack --mode production",
    "compile": "tsc -p ./",
    "webpack": "webpack --mode development",
    "info": "webpack --display-modules",
    "watch": "webpack --mode development --watch",
    "lint": "tslint -p ./"
  },
  "dependencies": {
    "copy-webpack-plugin": "^10.2.4",
    "decompress": "4.2.1",
    "maven": "^5.0.0"
  },
  "devDependencies": {
    "@types/vscode": "=1.49.0",
    "@types/xml2js": "^0.4.8",
    "ts-loader": "^6.2.1",
    "tslint": "^6.1.0",
    "typescript": "^3.9.10",
    "vsce": "^2.6.4",
    "webpack": "^5.40.0",
    "webpack-cli": "^4.7.2"
  },
  "extensionDependencies": [
    "asf.apache-netbeans-java"
  ]
}
