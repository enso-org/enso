    "@vueuse/core": "^10.4.1",
    "ag-grid-community": "^30.2.1",
    "ag-grid-enterprise": "^30.2.1",
    "codemirror": "^6.0.1",
    "culori": "^3.2.0",
    "enso-dashboard": "^0.1.0",
    "events": "^3.3.0",
    "fast-diff": "^1.3.0",
    "hash-sum": "^2.0.0",
    "install": "^0.13.0",
    "isomorphic-ws": "^5.0.0",
    "lib0": "^0.2.85",
